 props ]\r\n\r\n  const handleInputChange = event => {\r\n    const { name, value } = event.target\r\n\r\n    setUser({ ...user, [name]: value })\r\n  }\r\n\r\n  return (\r\n    <form\r\n      onSubmit={event => {\r\n        event.preventDefault()\r\n\r\n        props.updateUser(user.id, user)\r\n      }}\r\n    >\r\n      <label>Name</label>\r\n      <input type=\"text\" name=\"name\" value={user.name} onChange={handleInputChange} />\r\n      <label>Username</label>\r\n      <input type=\"text\" name=\"username\" value={user.username} onChange={handleInputChange} />\r\n      <button class=\"btn btn-info\">Update tache</button>\r\n      <button class=\"btn btn-secondary\" onClick={() => props.setEditing(false)} class=\"btn btn-secondary\">\r\n        Cancel\r\n      </button>\r\n    </form>\r\n  )\r\n}\r\n\r\nexport default EditUserForm","C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\index.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\App.js",["106","107","108","109","110","111"],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Home.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Dashboard.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\forms\\EditUserForm.js",["112"],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\tables\\UserTable.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Utils\\PublicRoute.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Utils\\Common.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Utils\\PrivateRoute.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\forms\\AddUserForm.js",[],"C:\\Users\\Med\\Desktop\\My-Github\\to-do-list-react\\src\\Login.js",["113","114"],{"ruleId":"115","replacedBy":"116"},{"ruleId":"117","replacedBy":"118"},{"ruleId":"119","severity":1,"message":"120","line":6,"column":8,"nodeType":"121","messageId":"122","endLine":6,"endColumn":20},{"ruleId":"119","seve